@font-face
  font-family 'Nunito'
  src url('../fonts/Nunito-Black.woff2') format('woff2')
  font-weight 900
  font-style black

body
  position fixed
  top 0
  left 0
  display flex
  justify-content center
  width 100%
  height 100%
  margin 0
  padding 0
  overflow hidden
  background-color #0F335A

canvas
  -webkit-touch-callout none
  -webkit-user-select none
  -khtml-user-select none
  -moz-user-select none
  -ms-user-select none
  user-select none
			